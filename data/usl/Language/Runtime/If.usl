val true := [
	def select (ifTrue, _) := ifTrue
	def not := false
	def and(that) := that
	def or (_) := true
	def xor (that) := that select (false, true)
]

val false := [
	def select (_, ifFalse) := ifFalse
	def not := true
	def and (that) := false
	def or (that) := that
	def xor (that) := that select (true, false)
]
